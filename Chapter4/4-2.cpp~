#include<iostream>
#include<string>
#include<math.h>
using std::cin;
using std::cout;
using std::endl;
using std::string;
int main() {
  int maxlen = 3;
  for (int i=1; i<=100; i++) {
    int len = floor( log10( i ) ) + 1;
    cout << i << string(maxlen + 1 - len, ' ') << i*i;
    cout << endl;
  }
  return 0;
} 
  
