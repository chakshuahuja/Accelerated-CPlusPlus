#include<iostream>
#include<string>
#include<math.h>
#include<iomanip>
using std::cin;
using std::cout;
using std::endl;
using std::string;
using std::setw;
int main() {
  int maxlen = 3;
  for (int i=1; i<=100; i++) {
    int len = floor(log10(i)) + 1;
    cout << i << setw(maxlen + 1) << i*i;
    cout << endl;
  }
  return 0;
}



